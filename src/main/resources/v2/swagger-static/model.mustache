{{#models}}
{{#model}}

<h2>{{classname}}</h2>
  {{#vars}}
<ul class="parameter">
  <li class="param-required-{{required}}">{{name}} : {{datatype}}
    <br/>{{description}}
    {{#is this 'enum'}}
      <dl class="param-enum">
        <dt>Enum:
          {{#_enum}}
            <dd>{{this}}</dd>
          {{/_enum}}
        </dt>
      </dl>
    {{/is}}
  </li>
</ul>
  {{/vars}}
{{/model}}
{{/models}}
